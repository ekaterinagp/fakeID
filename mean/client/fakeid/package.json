{
  "name": "fakeid",
  "version": "0.0.0",
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng run fakeid:e2e-ci",
    "e2e:coverage": "npx nyc report --reporter=lcov --reporter=text-summary",
    "e2e:ci:coverage": "npm run e2e:ci && npm run e2e:coverage",
    "heroku-postbuild": "ng build --prod",
    "cypress": "cypress open",
    "postinstall": "ngcc",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/fakeid/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/cli": "^11.0.5",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/material-moment-adapter": "^11.0.3",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "typescript": "~4.0.2",
    "zone.js": "~0.10.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@briebug/cypress-schematic": "^4.4.0",
    "@cypress/code-coverage": "^3.8.8",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^12.19.12",
    "codelyzer": "^6.0.0",
    "cypress": "^6.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^11.0.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-loader": "8.0.12",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^4.3.0"
  }
